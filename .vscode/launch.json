	"configurations": [
		{
			"name": "Pack-CssFiles",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/.customFeedPreview/Pack-CssFiles.ps1",
		},
		{
			"name": "PowerShell Launch Current File",
			"type": "PowerShell",
			"request": "launch",
			"script": "${file}",
			"cwd": "${file}"
		},
		{
			"name": "Launch Sage-Like",
			"type": "firefox",
			"request": "launch",
			"reAttach": true,
			"addonPath": "${workspaceFolder}/Sage-Like",

